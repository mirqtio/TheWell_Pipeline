    "@mui/x-charts": "^6.18.3",
    "@mui/x-data-grid": "^6.18.3",
    "@mui/x-date-pickers": "^6.18.3",
    "@reduxjs/toolkit": "^1.9.7",
    "date-fns": "^2.30.0",
    "axios": "^1.6.0",
    "body-parser": "^1.20.2",
    "bull": "^4.12.0",
    "canvas": "^2.11.2",
    "chart.js": "^4.4.1",
    "chokidar": "^3.5.3",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "handlebars": "^4.7.8",
    "jaeger-client": "^3.19.0",
    "joi": "^17.9.2",
    "json2csv": "^5.0.7",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "node-schedule": "^2.1.1",
    "opentracing": "^0.14.7",
    "pdfkit": "^0.14.0",
    "pg": "^8.16.0",
    "pg-hstore": "^2.3.4",
    "pgvector": "^0.1.8",
    "prom-client": "^15.1.3",
    "redis": "^4.6.0",
    "ioredis": "^5.3.2",
    "sequelize": "^6.37.7",
    "socket.io": "^4.7.5",
    "@tensorflow/tfjs-node": "^4.17.0",
    "natural": "^6.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "task-master-ai": "^0.15.0",
    "uuid": "^9.0.1",
    "winston": "^3.10.0",
    "puppeteer": "^21.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.20.1",
    "recharts": "^2.10.3",
    "socket.io-client": "^4.6.1",
    "classnames": "^2.3.2",
    "framer-motion": "^10.16.4",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "react-modal": "^3.16.1",
    "focus-trap-react": "^10.2.3",
    "@floating-ui/react": "^0.25.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/react-redux": "^7.1.33",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.44.0",
    "jest": "^29.6.1",
    "jsdom": "^26.1.0",
    "nodemon": "^3.0.1",
    "@playwright/test": "^1.52.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.6.0",
    "ioredis-mock": "^8.9.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.7.6",
    "redis-memory-server": "^0.9.0",
    "style-loader": "^3.3.3",
    "supertest": "^6.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0",
    "@storybook/addon-essentials": "^7.6.0",
    "@storybook/addon-interactions": "^7.6.0",
    "@storybook/addon-a11y": "^7.6.0",
    "@storybook/addon-viewport": "^7.6.0",
    "@storybook/addon-docs": "^7.6.0",
    "@storybook/react": "^7.6.0",
    "@storybook/react-webpack5": "^7.6.0",
    "@storybook/test-runner": "^0.13.0",
    "storybook": "^7.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "data-ingestion",
    "knowledge-management",
    "rag",
    "llm",
    "vector-database"
  ],
  "author": "TheWell Pipeline Team",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/**/index.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "testTimeout": 20000,
    "maxWorkers": 2,
    "forceExit": true,
    "detectOpenHandles": true,
    "verbose": true,
    "silent": false
  }
}
